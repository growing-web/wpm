{
  "name": "@growing-web/pnpm-nodemodules-provider",
  "version": "0.3.1",
  "description": "Package Import Map Generation Tool",
  "keywords": [
    "jspm",
    "import maps",
    "es modules",
    "cdn",
    "package manager"
  ],
  "homepage": "https://github.com/growing-web/nodemodules-provider#readme",
  "bugs": {
    "url": "https://github.com/growing-web/nodemodules-provider/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/growing-web/nodemodules-provider.git"
  },
  "license": "MIT",
  "author": "Guy Bedford",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.cjs",
      "import": "./dist/index.mjs"
    }
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "lib"
  ],
  "scripts": {
    "build": "unbuild",
    "stub": "unbuild --stub",
    "prepublishOnly": "unbuild"
  },
  "dependencies": {
    "@babel/core": "^7.21.8",
    "@babel/plugin-syntax-import-assertions": "^7.20.0",
    "@babel/preset-typescript": "^7.21.5",
    "@jspm/generator": "1.1.7",
    "@jspm/import-map": "^1.0.7",
    "@manypkg/get-packages": "^2.1.0",
    "sver": "^1.8.4"
  },
  "devDependencies": {
    "typescript": "^5.0.4",
    "@types/node": "^20.2.1",
    "chalk": "^5.2.0",
    "cross-env": "^7.0.3",
    "rimraf": "^5.0.1",
    "typedoc": "^0.24.7",
    "unbuild": "^1.2.1"
  },
  "publishConfig": {
    "access": "public"
  }
}
