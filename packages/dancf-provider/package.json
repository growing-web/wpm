{
  "name": "@growing-web/dancf-provider",
  "version": "0.4.2",
  "description": "Package Import Map Generation Tool",
  "keywords": [
    "jspm",
    "import maps",
    "es modules",
    "cdn",
    "package manager"
  ],
  "homepage": "https://github.com/growing-web/dancf-provider#readme",
  "bugs": {
    "url": "https://github.com/growing-web/dancf-provider/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/growing-web/dancf-provider.git"
  },
  "license": "MIT",
  "author": "Guy Bedford",
  "type": "module",
  "imports": {
    "#fetch": {
      "source": {
        "vscode": "./dist/common/fetch-vscode.mjs",
        "deno": "./dist/common/fetch-deno.mjs",
        "node": "./dist/common/fetch-node.mjs",
        "default": "./dist/common/fetch-native.mjs"
      },
      "default": {
        "vscode": "./dist/fetch-vscode.mjs",
        "deno": "./dist/fetch-deno.mjs",
        "node": "./dist/fetch-node.mjs",
        "default": "./dist/fetch-native.mjs"
      }
    },
    "#test/*": {
      "source": "./test/*.js"
    }
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.cjs",
      "import": "./dist/index.mjs"
    }
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "lib"
  ],
  "scripts": {
    "build": "unbuild",
    "prepublishOnly": "npm run build"
  },
  "dependencies": {
    "@jspm/core": "^2.0.1",
    "@jspm/generator": "1.1.7",
    "@jspm/import-map": "^1.0.7",
    "es-module-lexer": "^1.2.1",
    "ipfs-client": "^0.10.0",
    "make-fetch-happen": "^11.1.1",
    "node-fetch": "^3.3.1",
    "sver": "^1.8.4",
    "rimraf": "^5.0.1"
  },
  "devDependencies": {
    "typescript": "^5.0.4",
    "@swc/cli": "^0.1.62",
    "@swc/core": "^1.3.59",
    "chalk": "^5.2.0",
    "cross-env": "^7.0.3",
    "ipfs-utils": "^9.0.14",
    "kleur": "^4.1.5",
    "lit-element": "^3.3.2",
    "mocha": "^10.2.0",
    "open": "^9.1.0",
    "rollup": "^3.22.0",
    "typedoc": "^0.24.7",
    "unbuild": "^1.2.1"
  },
  "publishConfig": {
    "access": "public"
  }
}
