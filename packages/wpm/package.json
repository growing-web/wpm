{
  "name": "@growing-web/wpm",
  "version": "0.6.4",
  "description": "A package manager tool based on importmap",
  "license": "MIT",
  "author": "aui",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.cjs",
      "import": "./dist/index.mjs"
    }
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "bin": {
    "wpm": "./bin/wpm.mjs"
  },
  "scripts": {
    "build": "unbuild",
    "stub": "unbuild --stub",
    "prepublishOnly": "pnpm run build"
  },
  "files": [
    "dist"
  ],
  "dependencies": {
    "@growing-web/dancf-provider": "workspace:*",
    "@growing-web/esmpack-builder": "^0.4.3",
    "@growing-web/pnpm-nodemodules-provider": "workspace:*",
    "@jspm/generator": "1.1.7",
    "@jspm/import-map": "^1.0.7",
    "@manypkg/get-packages": "^2.1.0",
    "cac": "^6.7.14",
    "consola": "^3.1.0",
    "fast-glob": "^3.2.12",
    "find-up": "^6.3.0",
    "fs-extra": "^11.1.1",
    "json-schema-ref-parser": "^9.0.9",
    "lodash.merge": "^4.6.2",
    "picocolors": "^1.0.0",
    "pkg-types": "^1.0.3",
    "rollup": "3.22.0",
    "semver": "^7.5.1",
    "single-line-log": "^1.1.2",
    "undici": "5.22.1"
  },
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "@types/fs-extra": "^11.0.1",
    "@types/semver": "^7.5.0",
    "@types/single-line-log": "^1.1.0",
    "unbuild": "^1.2.1"
  },
  "engines": {
    "node": ">=16.0.0"
  }
}
